#event-list.row
  .widget.stacked
    .widget-header
      %h3
        %i.icon-calendar
        Events
    .widget-content.min-height-200
      .row
        .col-sm-12.text-right
          =# link_to "New Event", "#add-event", class: "btn btn-primary", data: { toggle: "modal" }
        = render "new"
      .row
        .col-sm-12
          %table.table.table-bordered.table-striped
            %thead
              %tr
                %th Name
                %th Date
                %th Ticket Url
                %th Ticket Price
                %th
            %tbody
              %tr
                %td
                %td
                %td
                %td
                %td