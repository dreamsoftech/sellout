- if user_signed_in?
  %li.dropdown
    %a.dropdown-toggle{"data-toggle" => "dropdown", href: "javscript:;"}
      %i.icon-cog
      Setting
      %b.caret
    %ul.dropdown-menu
      %li
        %a{href: "javascript:;"} Privacy Settings
      %li.divider
      %li
        %a{href: "javascript:;"} Help
  %li.dropdown
    %a.dropdown-toggle{"data-toggle" => "dropdown", href: "javscript:;"}
      %i.icon-user
      = current_user.name
      %b.caret
    %ul.dropdown-menu
      %li= link_to 'My Profile', edit_registration_path(current_user)
      %li.divider
      %li= link_to 'Logout', destroy_user_session_path, :method=>'delete'
- else
  %li
    %a{href: "/blog"} Blog
  %li
    %a{href: "/faq"} Faq
  %li
    %a{href: "/contact"} Contact
  %li
    %a{href: "/about"} About Us
  %li
    = link_to 'Sign up', new_user_registration_path
  %li
    = link_to 'Login', new_user_session_path